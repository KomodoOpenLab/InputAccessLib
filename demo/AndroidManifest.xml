<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demo.accessible"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="5" android:targetSdkVersion="9"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        
        <activity
            android:name=".AccessibleComponents"
            android:label="@string/accessible_app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity 
            android:name=".AlertDialogActivity"
            android:label="@string/alert_dialog_activity_name">
        </activity>

        <activity 
            android:name=".ListPreferenceActivity"
            android:label="@string/list_preference_activity_name">
        </activity>
        
        <activity 
            android:name=".OptionsMenuActivity"
            android:label="@string/options_menu_activity_name">
        </activity>

        <activity 
            android:name=".SpinnerActivity"
            android:label="@string/spinner_activity_name">
        </activity>

        <activity
            android:name="com.demo.inaccessible.InaccessibleComponents"
            android:label="@string/inaccessible_app_name" >
			<intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.demo.inaccessible.AlertDialogActivity"
            android:label="@string/inaccessible_alert_dialog_activity_name">
        </activity>

        <activity 
            android:name="com.demo.inaccessible.ListPreferenceActivity"
            android:label="@string/list_preference_activity_name">
        </activity>
        
        <activity 
            android:name="com.demo.inaccessible.OptionsMenuActivity"
            android:label="@string/inaccessible_options_menu_activity_name">
        </activity>

        <activity 
            android:name="com.demo.inaccessible.SpinnerActivity"
            android:label="@string/inaccessible_spinner_activity_name">
        </activity>
        
	</application>

</manifest>